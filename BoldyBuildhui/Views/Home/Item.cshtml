@model List<BoldyBuildhui.Models.Item>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ItemView</title>
</head>
<body>
    <div> 
        @foreach (var item in Model)
